\begin_inset Formula 
\begin{align*}
u_{*}\left(t\right) & =\left(\begin{array}{c}
\sin t\sqrt{4\sin^{2}t+1}\\
\frac{2\left(6\sin t-\sin3t\right)}{12\sin2t+2\sin4t+11}
\end{array}\right)\\
w & :=u-u_{*}\left(t\right)\\
w_{1} & =u_{1}-\sin\tau\sqrt{4\sin^{2}\tau+1}\\
w_{2} & =u_{2}-\frac{12\sin\tau-2\sin3\tau}{12\sin2\tau+2\sin4\tau+11}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Если подставить 
\begin_inset Formula 
\begin{align*}
\dot{\tau} & =\frac{\left(\rho_{1}+1\right)^{2}\cos^{2}\tau-\left(\rho_{1}+1\right)\sin\tau\ddot{x}}{\left(\rho_{1}+1\right)^{2}}\\
 & =\cos^{2}\tau+\frac{\sin\tau\sin\psi}{\rho_{1}+1}u_{1}\\
 & =\cos^{2}\tau+\frac{\sin\tau}{\rho_{1}+1}\left(\frac{\cos\rho_{3}}{\sqrt{4\sin^{2}\tau+1}}+\frac{2\sin\tau\sin\rho_{3}}{\sqrt{4\sin^{2}\tau+1}}\right)\left(w_{1}+\sin\tau\sqrt{4\sin^{2}\tau+1}\right)\\
 & =\cos^{2}\tau+\frac{\sin^{2}\tau}{\rho_{1}+1}\left(\cos\rho_{3}+2\sin\tau\sin\rho_{3}\right)+\frac{\sin\tau\cos\rho_{3}+2\sin^{2}\tau\sin\rho_{3}}{\left(\rho_{1}+1\right)\sqrt{4\sin^{2}\tau+1}}w_{1}
\end{align*}

\end_inset


\end_layout


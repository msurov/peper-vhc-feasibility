\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\tau & :=\arctan\frac{x}{\dot{x}}\\
\rho_{1} & :=\left(x-x_{*}\left(\tau\right)\right)\sin\tau+\left(\dot{x}-\dot{x}_{*}\left(\tau\right)\right)\cos\tau\\
\rho_{2} & :=z-z_{*}\left(\tau\right)\\
\rho_{3} & :=\psi-\psi_{*}\left(\tau\right)\\
\rho_{4} & :=\dot{z}-\dot{z}_{*}\left(\tau\right)\\
\rho_{5} & :=\dot{\psi}-\dot{\psi}_{*}\left(\tau\right)
\end{align*}

\end_inset


\end_layout

